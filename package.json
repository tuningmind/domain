{
  "name": "tuningmind.net",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "gh-pages": "^1.0.0"
  },
  "devDependencies": {
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "^15.6.0",
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
